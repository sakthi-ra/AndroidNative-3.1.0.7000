<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Android Native project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
            <dependency>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>android-phresco-plugin</artifactId>
                <type>jar</type>
                <version>3.1.0.7000</version>
                <class>com.photon.phresco.plugins.android.AndroidPlugin</class>
            </dependency>
        </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">SDK</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.TechnologyVersionImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>3.1.0.7000</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdk</key>
                    <multiple>false</multiple>
                    <value>2.3.3</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>3.1.0.7000</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <value>Test</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>true</key>
                            <value>-DskipTests=true</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>false</key>
                            <value>-DskipTests=false</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Skip Unit Test</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>skipTest</key>
                    <multiple>false</multiple>
                    <value>true</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>coverage</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Coverage</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>coverage</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Proguard</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>proguard</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Signing</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>signing</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>keystore,storepass,keypass,alias</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">ZipAlign</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>zipAlign</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>keystore,storepass,keypass,alias,signing</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Keystore</value>
                    </name>
                    <type>FileBrowse</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>keystore</key>
                    <multiple>false</multiple>
                    <value>D:\phresco-framework-3.1.0.7000-dist-testing\phresco-framework\workspace\projects\Androidnative\phresco.keystore</value>
                    <sort>false</sort>
                    <show>false</show>
                    <fileType>keystore</fileType>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Storepass</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>storepass</key>
                    <multiple>false</multiple>
                    <value>123456789</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Keypass</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>keypass</key>
                    <multiple>false</multiple>
                    <value>123456789</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Alias</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>alias</key>
                    <multiple>false</multiple>
                    <value>phresco</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>showErrors</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
